<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>Cars IF</title>
    <link rel="stylesheet" href="/styles/basicStyles.css" />
  </head>
  <body>
    <h1>All Cars</h1>
    <% if(cars.length===0){ %>
    <h3>No cars found!</h3>
    <%} else { %>
    <table>
      <thead>
        <tr>
          <th>Mark</th>
          <th>Licence</th>
        </tr>
      </thead>
      <tbody>
        <% cars.forEach(car=>{ %>
        <tr>
          <td><%= car.model %></td>
          <td><%= car.licence %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
    <% } %>
    <a href="/"><button>Home</button></a>
  </body>
</html>
